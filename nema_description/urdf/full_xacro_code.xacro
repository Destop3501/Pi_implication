<?xml version="1.0"?>
<robot name="NEma" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <link name="world"></link>

    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.050" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <cylinder radius="0.200" length="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.050" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.200" length="0.1"/>
            </geometry>
            <material name="red">
                <color rgba="0 0 1 1"/>
            </material>
        </collision>
        <inertial>
            <origin xyz="0 0 0.050" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
    </link>

    <link name="body_link">
        <visual>
            <origin xyz="0.0 -0.05 -0.010" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <mesh filename="package://nema_description/3D/Mesh/Body.dae" scale="0.002 0.002 0.002"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.0 -0.05 -0.010" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Collition/Body.stl" scale="0.002 0.002 0.002"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.4"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.4"/>
        </inertial>
    </link>

    <link name="face_link">
        <visual>
            <origin xyz="0.0 0.050 -0.470" rpy="0.0 0.0 3.14"/> 
            <geometry>
                <mesh filename="package://nema_description/3D/Mesh/Face.dae" scale="0.002 0.002 0.002"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.0 0.050 -0.470" rpy="0.0 0.0 3.14"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Collition/Face.stl" scale="0.002 0.002 0.002"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.075" rpy="0.0 0.0 0.0"/>
            <mass value="0.08"/>
            <inertia ixx="0.08" ixy="0.0" ixz="0.0" iyy="0.08" iyz="0.0" izz="0.08"/>
        </inertial>
    </link>

    <link name="face_intermediate_link"></link>

    <link name="right_arm">
        <visual>
            <origin xyz="-0.12 -0.02 -0.35" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Mesh/RightArm.dae" scale="0.002 0.002 0.002"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.12 -0.02 -0.35" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Collition/RightArm.stl" scale="0.002 0.002 0.002"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.0 0.0 -0.075" rpy="0.0 0.0 0.0"/>
            <mass value="0.06"/>
            <inertia ixx="0.06" ixy="0.0" ixz="0.0" iyy="0.06" iyz="0.0" izz="0.06"/>
        </inertial>
    </link>

    <link name="right_arm_intermediate_link">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 3.14"/>
    </link>
    <link name="left_arm_intermediate_link">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 3.14"/>
    </link>

    <link name="left_arm">
        <visual>
            <origin xyz="0.24 0 -0.38" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Mesh/LeftArm.dae" scale="0.002 0.002 0.002"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.24 0 -0.38" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://nema_description/3D/Collition/LeftArm.stl" scale="0.002 0.002 0.002"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.0 0.0 -0.0375" rpy="0.0 0.0 0.0"/>
            <mass value="0.03"/>
            <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
        </inertial>
    </link>
    
    <joint name="fixed" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <joint name="body_joint" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-0.8" soft_upper_limit="0.8"/>
        <origin rpy="0 0 0" xyz="0 0 0.100"/>
        <parent link="base_link"/>
        <child link="body_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="5" lower="-0.85" upper="0.85" velocity="0.3"/>
    </joint>

    <joint name="face_joint_z" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-0.8" soft_upper_limit="0.8"/>
        <origin rpy="0 0 0" xyz="0 0 0.400"/>
        <parent link="body_link"/>
        <child link="face_intermediate_link"/>
        <axis xyz="0 0 1"/> 
        <limit effort="30.0" lower="-0.85" upper="0.85" velocity="10.0"/>
    </joint>

    <joint name="face_joint_x" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-0.8" soft_upper_limit="0.8"/>
        <origin rpy="0 0 0" xyz="0 0 0.0"/> 
        <parent link="face_intermediate_link"/>
        <child link="face_link"/>
        <axis xyz="1 0 0"/> 
        <limit effort="5" lower="-0.85" upper="0.85" velocity="0.25"/>
    </joint>
    <joint name="right_joint_y" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="0" soft_upper_limit="0.4"/>
        <origin rpy="0.0 0 0" xyz="0.125 -0.03 0.300"/>
        <parent link="body_link"/>
        <child link="right_arm_intermediate_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="30.0" lower="-0.05" upper="0.45" velocity="10.0"/>
    </joint>
    <joint name="right_joint_x" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="0" soft_upper_limit="0.8"/>
        <origin rpy="0.0 0 0" xyz="0.0 0 0.0"/>
        <parent link="right_arm_intermediate_link"/>
        <child link="right_arm"/>
        <axis xyz="1 0 0"/>
        <limit effort="30.0" lower="-0.05" upper="0.85" velocity="10.0"/>
    </joint>
    <joint name="left_joint_y" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="0" soft_upper_limit="0.8"/>
        <origin rpy="0.0 0 0" xyz="-0.125 -0.03 0.300"/>
        <parent link="body_link"/>
        <child link="left_arm_intermediate_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="30.0" lower="-0.05" upper="0.85" velocity="10.0"/>
    </joint>
    <joint name="left_joint_x" type="revolute">
        <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="0" soft_upper_limit="0.4"/>
        <origin rpy="0.0 0 0" xyz="0.0 0 0.0"/>
        <parent link="left_arm_intermediate_link"/>
        <child link="left_arm"/>
        <axis xyz="1 0 0"/>
        <limit effort="30.0" lower="-0.05" upper="0.45" velocity="10.0"/>
    </joint>
</robot>